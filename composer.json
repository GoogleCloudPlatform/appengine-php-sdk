{
    "name": "google/appengine-php-sdk",
    "description": "Google App Engine PHP SDK",
    "type": "library",
    "keywords": ["google", "appengine", "sdk"],
    "homepage": "https://cloud.google.com/appengine/docs/php/",
    "license": "Apache-2.0",
    "support": {
        "issues": "https://code.google.com/p/googleappengine/wiki/FilingIssues?tm=3",
        "forum": "https://groups.google.com/forum/#!forum/google-appengine"
    },
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "~1",
        "phpunit/phpunit": "^8",
        "php-coveralls/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "google\\": "src"
        },
        "classmap": [
            "src/appengine/api/api_base_pb.php", 
            "src/appengine/api/memcache/memcache_service_pb.php", 
            "src/appengine/ext/remote_api/remote_api_pb.php",
            "src/appengine/api/app_identity/app_identity_service_pb.php" 
        ],
        "files": ["src/appengine/runtime/Memcache.php"]
      },
      "autoload-dev": {
        "psr-4": {
          "google\\tests\\": "tests"
        }
      }
}
